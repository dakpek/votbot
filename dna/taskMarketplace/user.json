{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "username",
    "vots"
  ],
  "properties": {
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The Username Schema",
      "default": "",
      "examples": [
        "dakpek"
      ],
      "pattern": "^(.*)$"
    },
    "vots": {
      "$id": "#/properties/vots",
      "type": "integer",
      "title": "The Vots Schema",
      "default": 0,
      "examples": [
        42
      ]
    }
  }
}